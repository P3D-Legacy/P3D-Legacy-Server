{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Server": {
    "IP": "0.0.0.0",
    "Port": 15124,
    "Name": "Put Server Name Here",
    "Message": "Put Server Description Here",
    "MaxPlayers": 1000,
    "ValidationEnabled": false,
    "IsOfficial": false
  },

  "P3D": {
    "APIEndpointV1": "",
    "APIToken": ""
  },

  "Discord": {
    "PasstroughChannelId": 0,
    "BotToken": ""
  },

  "Otlp": {
    "Enabled": true,
    "Host": "http://localhost:4317"
  },

